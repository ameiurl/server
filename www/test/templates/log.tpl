{if $ac == 'view'}{literal}<script type="text/javascript" src="/images/check.js"></script><style>    .td1{word-wrap:break-word;word-break:break-all;}</style>{/literal}<form name="form1" method="post" action="?ac=log&file=">    <input type="hidden" value="" name="file"><table width="100%" border="1" cellspacing="0" cellpadding="0" class="table2">      <tr>        <th width="10%">时间</th>        <!--<th width="8%" >IP</th>        <th width="20%">文件</th>-->               <th width="90%" >内容</th>              </tr>      {foreach from=$page_logs item=entry}      <tr>        <td>{$entry.dateline}</td>        <!--<td><?=$log['ip']?></td>        <td align="left" class="td1"><?=$log['link']?></td>-->        <td align="left" class="td1">{$entry.extra}</td>                 </tr>      {/foreach}</table>{$cutpage}</form>		{else}    {foreach from=$logfiles item=entry}    <li><a href="?action=log&todo=view&file={$entry}">{$entry}</a></li>    {/foreach}    {/if}